<?xml version="1.0"?>
<rdf:RDF xml:base="http://test.org/math.owl"
         xmlns="http://test.org/math.owl#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#">

<owl:Ontology rdf:about="http://test.org/math.owl"/>

<owl:Class rdf:about="#Statement">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Definition">
  <rdfs:subClassOf rdf:resource="#Statement"/>
</owl:Class>

<owl:Class rdf:about="#Theorem">
  <rdfs:subClassOf rdf:resource="#Statement"/>
</owl:Class>

<owl:Class rdf:about="#Lemma">
  <rdfs:subClassOf rdf:resource="#Theorem"/>
</owl:Class>

<Theorem rdf:about="#RN">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
</Theorem>


</rdf:RDF>
